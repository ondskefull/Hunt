	<script id="LightboxPhotoTemplate" class="template" type="text/template">
		<img src="{{imageUrl}}" height="{{imageHeight}}" >
	</script>

	<script id="LightboxHeaderTemplate" class="template" type="text/template">
		<button class="close wds-button wds-is-secondary"><svg class="wds-icon"><use xlink:href="#wds-icons-close"></use></svg></button>
		<a href="#" class="share-button wds-button wds-is-secondary">
			<svg class="wds-icon wds-icon-small"><use xlink:href="#wds-icons-share"></use></svg>			<span>Share</span>
		</a>
		{{#isUserAnon}}
		{{#imageDescription}}
		<div class="more-info-right wds-dropdown">
			<div class="wds-dropdown__toggle push-dropdown-down more-info-button wds-button wds-is-secondary">
				<svg class="wds-icon wds-icon-small"><use xlink:href="#wds-icons-question"></use></svg>				<span>More Info</span>
			</div>
			<div class="wds-dropdown__content wds-is-right-aligned more-info-container">
				{{{imageDescription}}}
			</div>
		</div>
		{{/imageDescription}}
		{{^imageDescription}}
			{{#externalUrl}}
			<a href="{{externalUrl}}" class="more-info-button wds-button wds-is-secondary">
				<svg class="wds-icon wds-icon-small"><use xlink:href="#wds-icons-question"></use></svg>				<span>More Info</span>
			</a>
			{{/externalUrl}}
		{{/imageDescription}}
		{{/isUserAnon}}
		{{^isUserAnon}}
		<a href="{{fileUrl}}" class="more-info-button wds-button wds-is-secondary">
			<svg class="wds-icon wds-icon-small"><use xlink:href="#wds-icons-question"></use></svg>			<span>More Info</span>
		</a>
		{{/isUserAnon}}
		<div id="lightbox-add-to-article" class="lightbox-add-to-article">
			<button class="article-add-button secondary">Add to article</button>
			<input class="lightbox-article-input" />
		</div>

		{{#isUserAnon}}
		{{#imageDescription}}
		<div class="more-info-dropdown more-info-left wds-dropdown">
			<div class="wds-dropdown__toggle push-dropdown-down">
				<h1>{{fileTitle}}</h1>
			</div>
			<div class="wds-dropdown__content wds-is-left-aligned more-info-container">
				{{{imageDescription}}}
			</div>
		</div>
		{{/imageDescription}}
		{{^imageDescription}}
		<h1>{{fileTitle}}</h1>
		{{/imageDescription}}
		{{/isUserAnon}}
		{{^isUserAnon}}
		<h1><a href="{{fileUrl}}">{{fileTitle}}</a></h1>
		{{/isUserAnon}}
		<a href="{{rawImageUrl}}" class="see-full-size-link">See full size image</a>
		<div class="user-details caption">
			{{#caption}}<p>{{caption}}</p>{{/caption}}
			{{#uploaderExists}}
			<img class="avatar" src="{{userThumbUrl}}">
			Added by <a href="{{userPageUrl}}" target="_blank">{{userName}}</a>			{{/uploaderExists}}
			{{#isPostedIn}}
				<span class="posted-in">
					Posted in {{#smallerArticleList}}<span class="posted-in-article"><a href="{{url}}" target="_blank">{{titleText}}</a></span>{{/smallerArticleList}}{{#articleListIsSmaller}}&hellip;{{/articleListIsSmaller}}				</span>
			{{/isPostedIn}}
		</div>
	</script>

