		<h1>Fast-adv</h1>
	</script>

	<script id="LightboxCarouselThumbs" type="text/template">
		{{#thumbs}}
			<li data-backfill="{{backfill}}">
				<a class="{{thumbWrapperClass}}">
					{{{playButtonSpan}}}
										<img class="thumb" src="data:image/gif;base64,R0lGODlhAQABAIABAAAAAP///yH5BAEAAAEALAAAAAABAAEAQAICTAEAOw%3D%3D" data-src="{{thumbUrl}}"
						 data-caption="{{caption}}" width="90"
						 height="55">
				</a>
			</li>
		{{/thumbs}}
	</script>

	<script id="LightboxCarouselMore" type="text/template">
		<li class="more-items disabled">
			<p class="subtle">{{{text}}}</p>
		</li>
	</script>

	<script id="LightboxCarouselProgressTemplate" type="text/template">
		<b>{{idx1}}-{{idx2}}</b> of <b>{{{total}}}</b>	</script>

	<script id="LightboxShareTemplate" type="text/template">
		<button class="close wds-button wds-is-secondary"><svg class="wds-icon"><use xlink:href="#wds-icons-close"></use></svg></button>
		<button class="more-info-close wds-button wds-is-secondary">
			<svg class="wds-icon wds-icon-small"><use xlink:href="#wds-icons-arrow"></use></svg>			<span>Back</span>
		</button>
		<div class="content">
			<div class="hero">
				<div class="hero-inner">
					<img src="{{imageUrl}}">
				</div>
			</div>
			<div class="share-form">
				<h1><a href="{{fileUrl}}" target="_blank">{{fileTitle}}</a></h1>
				<div class="wds-textarea">
					<div class="wds-textarea__field-wrapper">
						<label for="social-share-url">Standard link</label>
						<textarea rows="2" placeholder="Placeholder" maxlength="1000" id="social-share-url"
								  name="lightbox-standard-link"
								  class="wds-textarea__field ember-text-area ember-auto-resize ember-view">{{shareUrl}}</textarea>
					</div>
				</div>
				<div class="social-links">
					<ul>
						{{#networks}}
							<li>
								<a href="{{url}}" target="_blank" class="{{id}} wds-button wds-is-square
								wds-is-{{id}}-color">
									{{{icon}}}
								</a>
							</li>
						{{/networks}}
					</ul>
				</div>
			</div>
			<div class="bottom-forms">
				{{^isUserAnon}}
				<div class="more-links">
					<h2>URLs</h2>
					<div class="wds-textarea">
						<div class="wds-textarea__field-wrapper">
							<label for="social-share-file-url">File Page URL</label>
							<textarea rows="2" placeholder="Placeholder" maxlength="1000" id="social-share-file-url"
									  name="lightbox-file-page-url"
									  class="wds-textarea__field ember-text-area ember-auto-resize
									  ember-view">{{fileUrl}}</textarea>
						</div>
					</div>
				</div>
				{{/isUserAnon}}
				<div id="shareEmailForm" class="email">
					<h2>Email</h2>
					<div class="wds-textarea">
						<div class="wds-textarea__field-wrapper">
							<label for="social-share-file-url">Email this to a friend</label>
							<textarea rows="1" placeholder="" maxlength="1000" id="social-share-file-url"
									  name="lightbox-email"
									  class="wds-textarea__field ember-text-area ember-auto-resize
									  ember-view"></textarea>
						</div>
						<span class="wds-textarea__hint">Your friend's email</span>
					</div>
					<button id="lightboxSendEmailButton" class="wds-button wds-is-secondary">Send</button>
				</div>
			</div>
		</div>
	</script>

	<script id="LightboxErrorMessage" type="text/template">
